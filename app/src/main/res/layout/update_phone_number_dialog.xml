<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    xmlns:android="http://schemas.android.com/apk/res/android" />

    <android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/constraintLayout3"
        android:layout_width="275dp"
        android:layout_height="160dp"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/dialog_background"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.5">


        <EditText
            android:id="@+id/update_phone_numbe_dialog_editText_account"
            android:layout_width="264dp"
            android:layout_height="40dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/dialog_edit_text"
            android:hint="请输入新的手机号码"
            android:inputType="number|phone"
            android:singleLine="true"
            android:textColor="#f000"
            android:textColorHint="#cccccc"
            android:textSize="16sp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/update_phone_number_dialog_editText_code"
            android:layout_width="264dp"
            android:layout_height="40dp"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="12dp"
            android:background="@drawable/dialog_edit_text"
            android:hint="验证码"
            android:inputType="textPassword"
            android:password="true"
            android:textColor="#f000"
            android:textColorHint="#cccccc"
            android:textSize="16sp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/update_phone_numbe_dialog_editText_account" />

        <View
            android:id="@+id/view"
            android:layout_width="wrap_content"
            android:layout_height="0.8dp"
            android:layout_marginBottom="8dp"
            android:layout_marginTop="8dp"
            android:background="#acacac"
            app:layout_constraintBottom_toBottomOf="@+id/view2"
            app:layout_constraintTop_toBottomOf="@+id/update_phone_numbe_dialog_editText_account"
            app:layout_constraintVertical_bias="0.549"
            tools:layout_editor_absoluteX="-8dp" />

        <TextView
            android:id="@+id/update_phone_number_text_view_get_code"
            android:layout_width="86dp"
            android:layout_height="36dp"
            android:layout_marginBottom="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="38dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="108dp"
            android:gravity="center"
            android:text="获取验证码"
            android:textColor="#4990E2"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.296"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/update_phone_number_dialog_textView_Ok"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0" />

        <View

            android:id="@+id/view2"
            android:layout_width="0.8dp"
            android:layout_height="55dp"
            android:layout_marginLeft="0dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="64dp"
            android:background="#acacac"
            app:layout_constraintHorizontal_bias="0.675"
            app:layout_constraintLeft_toRightOf="@+id/update_phone_number_text_view_get_code"
            app:layout_constraintRight_toLeftOf="@+id/update_phone_number_dialog_textView_Ok"
            app:layout_constraintTop_toBottomOf="@+id/update_phone_numbe_dialog_editText_account"
            tools:layout_conversion_absoluteHeight="43dp"
            tools:layout_conversion_absoluteWidth="1dp"
            tools:layout_conversion_absoluteX="137dp"
            tools:layout_conversion_absoluteY="116dp" />

        <TextView
            android:id="@+id/update_phone_number_dialog_textView_Ok"
            android:layout_width="86dp"
            android:layout_height="36dp"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="157dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="108dp"
            android:gravity="center"
            android:text="确认更换"
            android:textColor="#4990E2"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.083"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0"
            tools:layout_conversion_absoluteHeight="43dp"
            tools:layout_conversion_absoluteWidth="137dp"
            tools:layout_conversion_absoluteX="138dp"
            tools:layout_conversion_absoluteY="116dp" />

    </android.support.constraint.ConstraintLayout>

    <ImageView
        android:id="@+id/update_phone_number_dialog_imageView_actionCancel"
        android:layout_width="28dp"
        android:layout_height="28dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="0dp"
        android:src="@mipmap/action_cancel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />
</android.support.constraint.ConstraintLayout>

